<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home</title>
  </head>
  <body>
    <div>
      <%- include('header') %> <%- include('sidebar', { records: records }) %>
      <h1>All records view</h1>
      <ul>
        <% locals.records.forEach(record => { %>
        <li><%= record.title %></li>
        <li><%= record.artist %></li>
        <li><%= record.yr %></li>
        <li><%= record.label %></li>
        <li><%= record.genre %></li>
        <% }) %>
      </ul>
    </div>
  </body>
</html>
